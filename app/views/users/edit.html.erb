<h2>User info</h2>
<%= form_with model: @user do |f| %>
  <div class="name">Name</div>
  <%= f.text_field :name %>
  <Div class="image">image</div>
  <%= f.file_field :profile_image, accept: "image/*" %>
  <div class="introduction">Introduction</div>
  <%= f.text_area :introduction %>
  <%= f.submit "Update Use" %>
<% end %>
